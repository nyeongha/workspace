package Control07;

public class TVUser {
	public static void main(String[] args) {
		LgTV tv=new LgTV();
		tv.turnOn();
		tv.channelDown();
		tv.channelUp();
		tv.channelUp();
		tv.channelUp();
		tv.soundUp();
		tv.soundUp();
		tv.soundDown();
		tv.soundUp();
		tv.turnOff();
		
		SamsungTV tv=new SamsungTV();
		tv.turnOn();
		tv.channelDown();
		tv.channelUp();
		tv.channelUp();
		tv.channelUp();
		tv.soundUp();
		tv.soundUp();
		tv.soundDown();
		tv.soundUp();
		tv.turnOff();
	}

}
