package oop.class01;

public class Point3D extends Point2D{
	private int z;
	
	public Point3D() {
		super();
		//반드시 부모클래스의 기본 생성자를 호출함
	}

	public int getZ() {
		return z;
	}

	public void setZ(int z) {
		this.z = z;
	}

	@Override
	public void print() {
		super.print();
		System.out.println("z="+getZ());
	}

}