package oop.class01;

public class Point2D {
	private int x;
	private int y;
	
	public Point2D() {
		//기본생성자
		x=1;
		y=2;
	}
	public Point2D(int x,int y) {
		this.x=x;
		this.y=y;
	}
	
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x=x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	
	public void print() {
		System.out.println("x="+getX());
		System.out.println("y="+getY());
	}


}
