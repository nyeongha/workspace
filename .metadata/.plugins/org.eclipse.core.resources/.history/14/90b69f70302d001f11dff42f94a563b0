package Control07;

public class TVUser {
	public static void main(String[] args) {
		LgTV tv=new LgTV();
		tv.turnOn();
		tv.channelDown();
		tv.channelUp();
		tv.channelUp();
		tv.channelUp();
		tv.soundUp();
		tv.soundUp();
		tv.soundDown();
		tv.soundUp();
		tv.turnOff();
		
		SamsungTV tv1=new SamsungTV();
		tv1.turnOn();
		tv1.channelDown();
		tv1.channelUp();
		tv1.channelUp();
		tv1.channelUp();
		tv1.soundUp();
		tv1.soundUp();
		tv1.soundDown();
		tv1.soundUp();
		tv1.turnOff();
	}

}
