package oop07.method;

public class DefaultConstructor {
	
	static class AC{
		int m;
		void work() {
			System.out.println(m);
		}
		
	}
	
	static class B{
		int m;
		void work() {
			System.out.println(m);
		}
		B(){
			
		}
	}
	static class C{
		int m;
		void work() {
			System.out.println(m);
		}
		C(int a){
			m=a;
		}
		
	}
	
	
	
	public static void main(String[] args) {
		
		
		AC ac= new AC();
		B b= new B();
		C c= new C(3);
		
		ac.work();
		b.work();
		c.work();
		
		
	}

}
